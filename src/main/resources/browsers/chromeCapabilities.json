{
	"browserName": "chrome",
	"goog:chromeOptions": {
		"args": [
			"--disable-infobars",
			"--ignore-certificate-errors",
			"--start-maximized",
			"--remote-allow-origins=*",
			"--disable-blink-features=AutomationControlled",
			"--no-sandbox",
			"--disable-dev-shm-usage"
		],
		"excludeSwitches": ["enable-automation"],
		"useAutomationExtension": false,
		"prefs": {
			"printing.enabled": false
		}
	},
	"platform": "ANY"
}